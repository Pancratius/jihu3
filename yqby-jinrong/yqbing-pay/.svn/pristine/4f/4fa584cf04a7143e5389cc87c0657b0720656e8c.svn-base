package com.yqbing.servicebing.webapp.request.pay.po;

import java.io.Serializable;

public class PayProvider implements Serializable{

	private static final long serialVersionUID = 1L;
	
	public static final int STATUS_ENABLED = 1;
	public static final int STATUS_DISABLED = 0;
	
	/**
	 * 支付提供商
	 */
	protected String payProvider;
	/**
	 * 支付提供商名称
	 */
	protected String payProviderName;
	/**
	 * 支付提供商描述
	 */
	protected String payProviderDesc;
	/**
	 * 状态：1-正常  0-停用
	 */
	protected Integer payProviderStatus;


	public String getPayProvider() {
		return payProvider;
	}

	public void setPayProvider(String payProvider) {
		this.payProvider = payProvider;
	}

	public String getPayProviderName() {
		return payProviderName;
	}

	public void setPayProviderName(String payProviderName) {
		this.payProviderName = payProviderName;
	}

	public String getPayProviderDesc() {
		return payProviderDesc;
	}

	public void setPayProviderDesc(String payProviderDesc) {
		this.payProviderDesc = payProviderDesc;
	}

	public Integer getPayProviderStatus() {
		return payProviderStatus;
	}

	public void setPayProviderStatus(Integer payProviderStatus) {
		this.payProviderStatus = payProviderStatus;
	}

	@Override
	public String toString() {
		return "PayProvider [payProvider=" + payProvider + ", payProviderName=" + payProviderName + ", payProviderDesc=" + payProviderDesc + ", payProviderStatus="
				+ payProviderStatus + "]";
	}
	
	
	
}
