package com.yqbing.servicebing.webapp.request.pay.po;

import java.io.Serializable;

/**
 * Title:PayChannel.java
 * Description:
 * @author alexaz
 * 2019年7月16日
 */
public class PayChannel implements Serializable{

	private static final long serialVersionUID = 1L;
	
	public static final int STATUS_ENABLED = 1;
	public static final int STATUS_DISABLED = 0;
	
	/**
	 * 支付提供商
	 */
	protected String payProvider;
	/**
	 * 支付渠道
	 */
	protected String payChannel;
	/**
	 * 支付渠道名称
	 */
	protected String payChannelName;
	/**
	 * 支付渠道描述
	 */
	protected String payChannelDesc;
	/**
	 * 支付渠道跳转url
	 */
	protected String payChannelPath;
	/**
	 * 状态：1-正常  0-停用
	 */
	protected Integer payChannelStatus;


	public String getPayProvider() {
		return payProvider;
	}

	public void setPayProvider(String payProvider) {
		this.payProvider = payProvider;
	}

	public String getPayChannel() {
		return payChannel;
	}

	public void setPayChannel(String payChannel) {
		this.payChannel = payChannel;
	}

	public String getPayChannelName() {
		return payChannelName;
	}

	public void setPayChannelName(String payChannelName) {
		this.payChannelName = payChannelName;
	}

	public String getPayChannelDesc() {
		return payChannelDesc;
	}

	public void setPayChannelDesc(String payChannelDesc) {
		this.payChannelDesc = payChannelDesc;
	}

	public String getPayChannelPath() {
		return payChannelPath;
	}

	public void setPayChannelPath(String payChannelPath) {
		this.payChannelPath = payChannelPath;
	}

	public Integer getPayChannelStatus() {
		return payChannelStatus;
	}

	public void setPayChannelStatus(Integer payChannelStatus) {
		this.payChannelStatus = payChannelStatus;
	}

	@Override
	public String toString() {
		return "PayChannel [payProvider=" + payProvider + ", payChannel=" + payChannel + ", payChannelName=" + payChannelName + ", payChannelDesc=" + payChannelDesc
				+ ", payChannelPath=" + payChannelPath + ", payChannelStatus=" + payChannelStatus + "]";
	}
	
	
	
}
