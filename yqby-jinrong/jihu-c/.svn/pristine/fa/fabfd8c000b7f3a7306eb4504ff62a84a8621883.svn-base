  <select id="queryByGame" resultMap="BaseResultMap" >
     select 
         id, 
         title, 
         img_url, 
         source, 
         link_url, 
         is_display_qrcode, 
         status, 
         createtime, 
         publishtime, 
         category_id,
         content,
         is_show_ios,
