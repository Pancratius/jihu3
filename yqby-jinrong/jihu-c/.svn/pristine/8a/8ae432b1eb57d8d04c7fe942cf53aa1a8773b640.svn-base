    function getDataList(st, et) {
        var codeJson = '{"token":"' + token + '","startTime":"' + st + '-00:00:00","endTime":"' + et + '-23:59:59","version":"' + version + '","appChannel":"' + appChannel + '"}';
        $.ajax({
            url: ONLINEURL + 'queryCloudPayTaskDetail.do?data=' + codeJson,
            type: 'post',
            dataType: 'JSON',
            success: function (data) {
                console.log(data);
                if (data.code == 0) {
                    $('.totalUserCntJinrong').html(data.body.totalTaskNum);
                    $('.totalValidCntJinrong').html(data.body.tasksEffectiveNum);
                } else {
                    $('.totalUserCntJinrong').html('0');
                    $('.totalValidCntJinrong').html('0');
                }
            }
        })
    }

    //当前日期
    function endTime() {
        var date = new Date;
        var year = date.getFullYear();
        var month = date.getMonth() + 1;
        var day = date.getDate();
        month = (month < 10 ? "0" + month : month);
        day = (day < 10 ? "0" + day : day);
        var nowDate = (year.toString() + '-' + month.toString() + '-' + day.toString());
        return nowDate
    }

    //当前日期前一个月
    function startTime() {
        var date = new Date;
        var year = date.getFullYear();
        var month = date.getMonth() + 1;
        month = (month < 10 ? "0" + month : month);
        var nowDate = (year.toString() + '-' + month.toString() + '-' + '01');
        return nowDate
    }

</script>

