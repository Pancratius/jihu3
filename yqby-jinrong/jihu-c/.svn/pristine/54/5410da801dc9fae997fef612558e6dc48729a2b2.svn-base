  <select id="queryInvite" resultMap="BaseResultMap">
    select 
    SUM(ud.settle_amount) as total,
    sh.app_pack as pack,
    sh.app_Name as name,
    sh.is_self as self,
    sh.app_Url_Img as img 
    from 
    
    user_down_data_day_detail ud 

    LEFT JOIN store_house sh on sh.app_pack = ud.app_pack  and sh.is_self !=0
    
    where 
    
    
    ud.invite_code in 
    <foreach collection="code" item="cid" index="index" open="(" close=")" separator=",">
	  #{cid}
	</foreach>
    
    and 
    
    ud.create_time &gt;= #{stime}
    
    and
    
    ud.create_time &lt;= #{etime}
    
    and    

    ud.status = "success"
    
    
    GROUP BY ud.app_pack
  </select>
