  <select id="predictList" resultMap="BaseResultMap"  >
   select 
   
   SUBSTRING(business_time,1,10) as workTime,
   
   sum(predict_amount) as  predictTotal, 
   
   sum(settle_amount)  as  settleTotal 
   
   from 
   
   game_cps_data_day_detail
     
   where 
   
   business_time &gt;= #{stime}
   
   and 
   
   business_time &lt;= #{etime}
   
   and 
   
   redeem_code in
   
    <foreach collection="code" item="cid" index="index" open="(" close=")" separator=",">
	  #{cid}
	</foreach>
   
   and 
	  
   (status !='fail' or status is null)
	  
   GROUP BY
    
   SUBSTRING(business_time,1,10)
   
   order by business_time desc
  </select>
  <select id="predictDatail" resultMap="BaseResultMap"  >
	  select 
	  
	  app_name,
	  
	  sum(predict_amount) as predictTotal,
	  
	  sum(settle_amount) as settleTotal 
	  
	  from
	   
	  game_cps_data_day_detail  
	  
	  where 
	  
	  business_time &gt;= #{stime} 
	  
	  and 
	  
	  business_time &lt;= #{etime}
	  
	  and redeem_code in
	  
	  <foreach collection="code" item="cid" index="index" open="(" close=")" separator=",">
		  #{cid}
	  </foreach>
	  
	  and 
	  
	 (status !='fail' or status is null)
	  
	  GROUP BY app_name
	  
	  order by business_time desc
