	        </where>
	     </foreach>
	    
	  </update> 
	  
	  
	  
	   <select id="queryTime" parameterType="java.lang.String" resultMap="BaseResultMap">
	         select 
	        <include refid="Base_Column_List" />
	         from 
	         
	         tb_we_bank 
	         where
	         date_format(be_invited_time, '%Y%m%d')  = DATE_FORMAT( #{stime} , '%Y%m%d')
	         and
	         is_successful = 3
	  </select>
