  <select id="queryWelth"   parameterType="java.lang.String" resultMap="BaseResultMap">
   select
	  tw.id,
	  tw.userId,
	  tw.invited_name,
	  tw.money,
	  tw.wealth,
	  tw.update_time,
	  ti.store_name as storename,
	  ti.user_phone as userphone,
	  ti.store_id as storeid,
	  ti.user_name as username 
	  from tb_we_bank tw 
	  LEFT JOIN tb_user_info ti  
	  on tw.userId = ti.user_id
	  <where>
          <if test="company != null and company != '' ">
               tw.invited_name like #{company,jdbcType=VARCHAR}
          </if>	        
          
          <if test="store != null and store != '' ">
             and  ti.store_name like #{store,jdbcType=VARCHAR}
          </if>	        
          
          <if test="stime != null">
             and  tw.update_time >= #{stime,jdbcType=TIMESTAMP}
               
          </if>	        
              
          <if test="etime != null">
             and  tw.update_time &lt;= #{etime,jdbcType=TIMESTAMP}
          </if>
          and tw.is_successful = 4
          
          and tw.existence = 0        
	  </where>
	  order by tw.update_time desc
	  </select>
 
	   <update id="updateBatch" parameterType="java.util.List" >
	    
