package com.yqbing.servicebing.webapp.response;

import java.util.List;

public class ProvinceRes {
	private Long id;
	private	String	name;
	private List<CityRes> city;
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
	public List<CityRes> getCity() {
		return city;
	}
	public void setCity(List<CityRes> city) {
		this.city = city;
	}
	
	
}
