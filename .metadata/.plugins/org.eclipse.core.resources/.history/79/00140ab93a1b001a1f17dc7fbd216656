package com.hiko.gym.common.repository.redis;

/**
 * 场馆统计标识接口
 * 
 * @author cychen
 *
 */
public interface RVenueStatisticsId {

	/**
	 * 递增场馆统计标识
	 * 
	 * @return 场馆统计标识
	 */
	public Long increment();

	/**
	 * 递增场馆统计标识
	 * 
	 * @param time 指定时间(毫秒)
	 * @return 场馆统计标识
	 */
	public Long increment(long time);

}
