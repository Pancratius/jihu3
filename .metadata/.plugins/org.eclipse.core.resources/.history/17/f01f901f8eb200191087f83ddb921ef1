package com.yqbing.servicebing.webapp.response;

import java.util.List;

public class ChargHomeRes {

	//套餐列表
	private List<EntryVo> entrys;
	//下载任务
	private List<BoxStoreAppRes> apps;
	//免费次数
	private Integer free;
	public List<EntryVo> getEntrys() {
		return entrys;
	}
	public void setEntrys(List<EntryVo> entrys) {
		this.entrys = entrys;
	}
	public List<BoxStoreAppRes> getApps() {
		return apps;
	}
	public void setApps(List<BoxStoreAppRes> apps) {
		this.apps = apps;
	}
	public Integer getFree() {
		return free;
	}
	public void setFree(Integer free) {
		this.free = free;
	}
	
	
}
