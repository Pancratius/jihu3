package com.yqbing.servicebing.repository.database.bean;

import java.util.Date;

public class AppBanner {
    private Long id;

    private String appCategoryId;

    private String imgurl;

    private String linkurl;

    private Integer sort;

    private Date createtime;

    private Byte status;

    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public String getAppCategoryId() {
        return appCategoryId;
    }

    public void setAppCategoryId(String appCategoryId) {
        this.appCategoryId = appCategoryId == null ? null : appCategoryId.trim();
    }

    public String getImgurl() {
        return imgurl;
    }

    public void setImgurl(String imgurl) {
        this.imgurl = imgurl == null ? null : imgurl.trim();
    }

    public String getLinkurl() {
        return linkurl;
    }

    public void setLinkurl(String linkurl) {
        this.linkurl = linkurl == null ? null : linkurl.trim();
    }

    public Integer getSort() {
        return sort;
    }

    public void setSort(Integer sort) {
        this.sort = sort;
    }

    public Date getCreatetime() {
        return createtime;
    }

    public void setCreatetime(Date createtime) {
        this.createtime = createtime;
    }

    public Byte getStatus() {
        return status;
    }

    public void setStatus(Byte status) {
        this.status = status;
    }
}